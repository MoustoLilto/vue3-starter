{
  "name": "etime",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 8080 --mode rec",
    "build:rec": "vue-tsc --noEmit && vite build --mode rec",
    "build:ppr": "vue-tsc --noEmit && vite build --mode ppr",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.js,.vue src test",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=junit --outputFile.junit=./test-report.xml",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vueuse/core": "^9.5.0",
    "axios": "^1.2.1",
    "axios-concurrency": "^1.0.4",
    "axios-retry": "^3.3.1",
    "luxon": "^3.1.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.24",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.1.25",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.9",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@unocss/reset": "^0.46.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-istanbul": "^0.25.3",
    "@vue/test-utils": "^2.2.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "jsdom": "^20.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "typescript": "4.8.4",
    "unocss": "^0.46.5",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.4",
    "vite-plugin-pages": "^0.27.1",
    "vitest": "^0.25.2",
    "vue-tsc": "^1.0.9"
  }
}
